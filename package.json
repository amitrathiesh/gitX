{
    "name": "gitx-manager",
    "productName": "gitX",
    "version": "0.1.0",
    "description": "Cross-platform repository management dashboard",
    "main": "electron/main.js",
    "scripts": {
        "dev": "concurrently \"npm run dev:vite\" \"npm run dev:electron\"",
        "dev:vite": "vite",
        "dev:electron": "wait-on tcp:5173 && electron .",
        "build": "vite build",
        "preview": "vite preview"
    },
    "keywords": [
        "electron",
        "git",
        "manager",
        "dashboard"
    ],
    "author": "Antigravity",
    "license": "MIT",
    "dependencies": {
        "electron-store": "^8.1.0",
        "lucide-react": "^0.294.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "xterm": "^5.3.0",
        "xterm-addon-fit": "^0.8.0"
    },
    "devDependencies": {
        "@vitejs/plugin-react": "^4.2.0",
        "autoprefixer": "^10.4.16",
        "concurrently": "^8.2.2",
        "electron": "^28.0.0",
        "postcss": "^8.4.31",
        "postcss-nesting": "^13.0.2",
        "sass": "^1.97.2",
        "tailwindcss": "^3.3.5",
        "vite": "^5.0.0",
        "wait-on": "^7.2.0"
    }
